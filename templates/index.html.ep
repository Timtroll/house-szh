<!DOCTYPE html>
<html>
<head>
  <title><%= $title %></title>
  <meta charset="UTF-8">
  <link rel="stylesheet" type="text/css" href="./css/style.css">
</head>
<body>
<div class="main">
  <div class="divtopleft"><span class="toptext">Cache</span></div>
  <div class="divtopright"><span class="toptext">Base</span></div>
  <div class="divmiddleleft" id="cache"></div>
  <div class="divmiddleright" id="base"></div>
  <div class="footer">
    <button class="add" data-id="0">Add</button>&nbsp;&nbsp;<button class="apply">Apply</button>&nbsp;&nbsp;<button class="load">Load to cache</button>&nbsp;&nbsp;<button class="reset">Reset</button>
  </div>

  <!-- The Modal -->
  <div id="myModal" class="modal">
    <div class="modal-content">
      <span class="close">&times;</span>
      <fieldset>
        <label for="name"></label>
        <input type="text" id="name" name="name" class="text">
      </fieldset>
      <input type="checkbox" id="status" name="status" class="text">
      <input type="hidden" name="id" class="text">
      <button class="save">Сохранить</button>
    </div>
  </div>
</div>
  <script type="text/javascript" charset="UTF-8"  src="./js/tree.js"></script>
</body>
</html>